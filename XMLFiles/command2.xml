<?xml version="1.0"?>
<!DOCTYPE PRODUCTION_ORDERS [
<!ELEMENT ORDERS (Request_Stores | xml.Order*)>
<!ELEMENT Request_Stores EMPTY>
<!ELEMENT xml.Order (Transform | Unload)>
<!ATTLIST xml.Order
          Number   (CDATA) #REQUIRED
>
<!ELEMENT Transform EMPTY>
<!ATTLIST Transform
          From     (CDATA) #REQUIRED
          To       (CDATA) #REQUIRED
          Quantity (CDATA) #REQUIRED
>
<!ELEMENT Unload EMPTY>
<!ATTLIST Unload
          Type        (CDATA) #REQUIRED
          Destination (CDATA) #REQUIRED
          Quantity    (CDATA) #REQUIRED
>
]>
<ORDERS>
<xml.Order Number="002">
<Transform From="P4" To="P5" Quantity="10"/>
</xml.Order>
<xml.Order Number="003">
<Transform From="P8" To="P9" Quantity="8"/>
</xml.Order>
</ORDERS>
